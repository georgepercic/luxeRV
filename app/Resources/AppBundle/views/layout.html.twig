<!doctype html>
<html class="no-js" lang="">

<head>
    <meta charset="utf-8">
    <title>{{ site_name }}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1, maximum-scale=1">
    <!-- page stylesheets -->
    {% block stylesheets %}
        <!-- end page stylesheets -->
        <link rel="stylesheet" href="{{ asset('assets/vendor/datatables/media/css/dataTables.bootstrap.css') }}">
        <!-- build:css({.tmp,app}) styles/app.min.css -->
        <link rel="stylesheet" href="{{ asset('assets/styles/webfont.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/styles/climacons-font.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/vendor/bootstrap/dist/css/bootstrap.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/styles/font-awesome.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/styles/card.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/styles/sli.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/styles/animate.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/styles/app.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/styles/app.skins.css') }}">

        <!-- endbuild -->
    {% endblock %}
</head>

<body class="page-loading">
<!-- page loading spinner -->
<div class="pageload">
    <div class="pageload-inner">
        <div class="sk-rotating-plane"></div>
    </div>
</div>
<!-- /page loading spinner -->
<div class="app layout-fixed-header">

    <!-- sidebar panel -->
    {% include 'AppBundle::_siderpanel.html.twig' %}
    <!-- /sidebar panel -->

    <!-- content panel -->
    <div class="main-panel">
        <!-- top header -->
        {% include 'AppBundle::_topheader.html.twig' %}
        <!-- /top header -->
        <!-- main area -->
        <div class="main-content">
            {% block body %}

            {% endblock %}
        </div>
        <!-- /main area -->
    </div>
    <!-- /content panel -->
    <!-- bottom footer -->
    <footer class="content-footer">
        {% include 'AppBundle::_footer.html.twig' %}
    </footer>
    <!-- /bottom footer -->
    <!-- chat -->
    {% include 'AppBundle::_chat.html.twig' %}
    <!-- /chat -->
</div>
{% block javascripts %}
    <!-- build:js({.tmp,app}) scripts/app.min.js -->
    <script src="{{ asset('assets/scripts/helpers/modernizr.js') }}"></script>
    <script src="{{ asset('assets/vendor/jquery/dist/jquery.js') }}"></script>
    <script src="{{ asset('assets/vendor/bootstrap/dist/js/bootstrap.js') }}"></script>
    <script src="{{ asset('assets/vendor/fastclick/lib/fastclick.js') }}"></script>
    <script src="{{ asset('assets/vendor/perfect-scrollbar/js/perfect-scrollbar.jquery.js') }}"></script>
    <script src="{{ asset('assets/scripts/helpers/smartresize.js') }}"></script>
    <script src="{{ asset('assets/scripts/constants.js') }}"></script>
    <script src="{{ asset('assets/scripts/main.js') }}"></script>
    <!-- endbuild -->
{% endblock %}
</body>

</html>